{
  "name": "@openzeppelin/ui-builder-adapter-evm-core",
  "version": "1.0.0",
  "private": true,
  "description": "Internal EVM core package for UI Builder adapters",
  "keywords": [
    "openzeppelin",
    "evm",
    "core",
    "adapter",
    "internal"
  ],
  "author": "OpenZeppelin <contact@openzeppelin.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@openzeppelin/relayer-sdk": "1.9.0",
    "@openzeppelin/ui-types": "^1.10.0",
    "@openzeppelin/ui-utils": "^1.3.0",
    "@wagmi/connectors": "5.7.13",
    "@wagmi/core": "^2.20.3",
    "lodash": "^4.17.21",
    "viem": "^2.33.3"
  },
  "peerDependencies": {
    "@openzeppelin/ui-components": "^1.0.0",
    "@openzeppelin/ui-react": "^1.0.0",
    "@rainbow-me/rainbowkit": "^2.0.0",
    "lucide-react": "^0.468.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@openzeppelin/ui-components": {
      "optional": true
    },
    "@openzeppelin/ui-react": {
      "optional": true
    },
    "@rainbow-me/rainbowkit": {
      "optional": true
    },
    "lucide-react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@openzeppelin/ui-components": "^1.3.0",
    "@openzeppelin/ui-react": "^1.0.0",
    "@rainbow-me/rainbowkit": "^2.0.0",
    "@types/lodash": "^4.17.20",
    "eslint": "^9.32.0",
    "lucide-react": "^0.468.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite-config": {
      "types": "./dist/vite-config.d.ts",
      "import": "./dist/vite-config.js",
      "require": "./dist/vite-config.cjs"
    }
  }
}
